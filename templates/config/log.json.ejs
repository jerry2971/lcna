{
  "winston_options": {
    "file_app": {
      "level": "info",
      "filename": "logs/app.log",
      "handleExceptions": true,
      "json": true,
      "maxsize": 5242880,
      "maxFiles": 5,
      "colorize": false
    },
    "console": {
      "level": "info",
      "handleExceptions": true,
      "json": false,
      "colorize": true
    }
  }<%if (rollbar) {%>,
  "rollbar_option": {
    "level": "error",
    "rollbarConfig": {
      "enabled": false,
      "accessToken": "ReplaceYourToken",
      "payload": {
        "fingerprint": "main_info"
      }
    }
  }<%}%>
}
